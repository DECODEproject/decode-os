command=/home/decode/go/bin/dam-dir
pidfile=/var/run/dam-dir.pid

description="Tor DAM server"

depend() {
	after logger
}

start() {
	start-stop-daemon --start -u decode -g decode \
		--pidfile $pidfile -b -m -x $command
}

command=/home/decode/go/bin/dam-client
pidfile=/var/run/dam-client.pid

description="Tor DAM client"

depend() {
	after dam-dir
}

start() {
	start-stop-daemon --start -u decode -g decode \
		--pidfile $pidfile -b -m -x $command
}
